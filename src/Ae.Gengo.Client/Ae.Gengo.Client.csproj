<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>1.0.0</Version>
    <Description>A client for https://gengo.com/ translation service for .NET Core.</Description>
    <Authors>alanedwardes</Authors>
    <WarningsAsErrors>true</WarningsAsErrors>
    <TargetFrameworks>netstandard2.0</TargetFrameworks>
    <PackageTags>gengo</PackageTags>
    <PackageIconUrl>https://avatars.githubusercontent.com/u/110954</PackageIconUrl>
    <PackageProjectUrl>https://github.com/alanedwardes/Ae.Gengo.Client</PackageProjectUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="2.2.0" />
    <PackageReference Include="Microsoft.Extensions.Http" Version="2.2.0" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.2" />
  </ItemGroup>

</Project>
